# ETF资产配置策略系统 - 环境配置示例
# 复制此文件为 .env 并修改相应配置

# 应用环境
ENVIRONMENT=development
DEBUG=True

# 数据库配置
DATABASE_URL=mysql+pymysql://etf_user:etf_password@localhost:3306/etf_strategy

# Redis配置
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=
REDIS_DB=0

# JWT认证配置
SECRET_KEY=your-secret-key-change-in-production
ACCESS_TOKEN_EXPIRE_MINUTES=43200

# Wind数据库配置
WIND_DB_HOST=your-wind-server-host
WIND_DB_PORT=1433
WIND_DB_NAME=wind_database
WIND_DB_USER=your-wind-username
WIND_DB_PASSWORD=your-wind-password

# 大语言模型配置
LLM_MODEL_TYPE=local
LLM_MODEL_NAME=qwen2-7b
LLM_API_BASE=http://localhost:11434
LLM_API_KEY=
LLM_MAX_TOKENS=2048
LLM_TEMPERATURE=0.7

# 资讯API配置
NEWS_API_BASE_URL=https://api.example-news.com
NEWS_API_KEY=your-news-api-key

# 日志配置
LOG_LEVEL=INFO
LOG_FILE=logs/app.log
